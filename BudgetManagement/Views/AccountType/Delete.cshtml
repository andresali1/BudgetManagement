@model AccountType

@{
    ViewData["Title"] = "Eliminar Tipo Cuenta";
}

<h3 class="mb-3">¿Está seguro que desea borrar el tipo de cuenta @Model.AT_Name?</h3>

<form asp-action="DeleteAccountType">
    <input type="hidden" asp-for="Id" />
    <button type="submit" class="btn btn-danger">Borrar</button>
    <a class="btn btn-secondary" asp-action="Index">Cancelar</a>
</form>
